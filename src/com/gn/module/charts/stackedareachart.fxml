                                 <children>
                                    <StackedAreaChart fx:id="stackedAreaChart" title="StackedAreaChart">
                                      <xAxis>
                                        <CategoryAxis side="BOTTOM" />
                                      </xAxis>
                                      <yAxis>
                                        <NumberAxis side="LEFT" />
                                      </yAxis>
                                    </StackedAreaChart>
                                 </children></HBox>
